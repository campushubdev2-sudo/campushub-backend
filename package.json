{
  "name": "campushub-server",
  "version": "0.0.0",
  "main": "src/app.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "nodemon --watch src --ext js,mjs,cjs --exec \"eslint .\"",
    "setup:uploads": "mkdirp uploads/reports/bylaws uploads/reports/proposal uploads/reports/financial uploads/reports/actionPlan",
    "postinstall": "npm run setup:uploads"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.2.1",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "mkdirp": "^3.0.1",
    "mongodb": "^7.1.0",
    "mongoose": "^9.1.5",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^17.1.0",
    "nodemon": "^3.1.11"
  }
